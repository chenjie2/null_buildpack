#!/usr/bin/env bash

echo "========> do nothing"



echo `pwd`
echo `ls`

BUILD_DIR=$1
CACHE_DIR=$2
echo $BUILD_DIR
echo $CACHE_DIR

echo `ls $BUILD_DIR/*`
echo `ls $CACHE_DIR/*`

echo "<============unzip tomcat"
cd $BUILD_DIR
tar -zxvf apache-tomcat-6.0.39.tar.gz 

echo `ls $BUILD_DIR/*`

wget -q http://download.run.pivotal.io/openjdk/lucid/x86_64/openjdk-1.7.0_51.tar.gz
mkdir -p openjdk
tar -zxvf openjdk-1.7.0_51.tar.gz openjdk


echo "<===============finsh"
exit 0
