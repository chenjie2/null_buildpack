#!/usr/bin/env bash

echo "========> do nothing"

BUILD_DIR=$1
CACHE_DIR=$2
mkdir -p $BUILD_DIR $CACHE_DIR

UAP_URL="http://223.203.195.147/file/UAP.zip"
JDK_URL="http://223.203.195.147/file/openjdk-1.6.0_22.tar.gz"

# download UAP,openjdk
cd ${BUILD_DIR}
echo "<===============download uap"
#curl --silent --max-time 60 --location $UAP_URL | unzip 

mkdir -p ./openjdk && cd ./openjdk
echo "<================download jdk"
#curl --silent --max-time 60 --location $JDK_URL
wget -q $JDK_URL
tar -xzvf openjdk-1.6.0_22.tar.gz
exit 0
